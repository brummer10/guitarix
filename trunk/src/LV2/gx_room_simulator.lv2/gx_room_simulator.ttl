@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:guitarix-developer@lists.sourceforge.net> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_room_simulator_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_room_simulator_" .

<http://guitarix.sourceforge.net/plugins/gx_room_simulator_#_room_simulator_>
    a lv2:Plugin ,
        lv2:ReverbPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "Gxroom_simulator";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_room_simulator_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
      
    guiext:ui <http://guitarix.sourceforge.net/plugins/gx_room_simulator_#gui>;
      
    lv2:minorVersion 28;
    lv2:microVersion 3;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "EFFECT" ;
        lv2:name "EFFECT" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "PREDELAYMS" ;
        lv2:name "PREDELAYMS" ;
        lv2:default 2e+01 ;
        lv2:minimum 1.0 ;
        lv2:maximum 2e+02 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "RT" ;
        lv2:name "RT" ;
        lv2:default 0.3 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "ROOMSIZE" ;
        lv2:name "ROOMSIZE" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 3.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "DRYWET" ;
        lv2:name "DRYWET" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] .
    
<http://guitarix.sourceforge.net/plugins/gx_room_simulator_#gui>
  a guiext:GtkUI;
  guiext:binary <gx_room_simulator_gui.so>;
  guiext:requiredFeature guiext:makeResident .
