@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix mod: <http://portalmod.com/ns/modgui#> .

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_fuzz_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_fuzz_" .

<http://guitarix.sourceforge.net/plugins/gx_fuzz_#fuzz_>
    a lv2:Plugin ,
        lv2:DistortionPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxFuzz";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_fuzz_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    
  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_fuzz_#gui>;
  
    lv2:minorVersion 28;
    lv2:microVersion 2;

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "INPUT" ;
        lv2:name "INPUT";
        lv2:default 0.0 ;
        lv2:minimum -20.0 ;
        lv2:maximum  1e+01 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "OUTPUT" ;
        lv2:name "OUTPUT";
        lv2:default 1e+02 ;
        lv2:minimum 5e+01 ;
        lv2:maximum 1e+02 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "DRIVE" ;
        lv2:name "DRIVE";
        lv2:default 1.0 ;
        lv2:minimum -3.0 ;
        lv2:maximum 100 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "TONE" ;
        lv2:name "TONE";
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a  lv2:ControlPort, lv2:OutputPort ;
        lv2:index 4 ;
        lv2:symbol "V1" ;
        lv2:name "V1";
        lv2:default 0.0 ;
        lv2:minimum -70.0 ;
        lv2:maximum 4.0 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 5 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] ;
    mod:gui [ 
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/gx_fuzz.html>;
        mod:templateData <modgui/gx_fuzz.json>;
        mod:screenshot <modgui/gx_fuzz.png>;
        mod:thumbnail <modgui/gx_fuzz-thumb.png>;
    ] .


<http://guitarix.sourceforge.net/plugins/gx_fuzz_#gui>
  a guiext:GtkUI;
  guiext:binary <gx_fuzz_gui.so>;
  guiext:requiredFeature guiext:makeResident .
  
