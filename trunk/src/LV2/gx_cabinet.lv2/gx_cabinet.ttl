<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_cabinet>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "GxCabinet" .

<http://guitarix.sourceforge.net/plugins/gx_cabinet#CABINET>
    a lv2:Plugin ,
        lv2:SimulatorPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxCabinet";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_cabinet> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:requiredFeature urid:map ,
        bufsz:boundedBlockLength ,
        work:schedule ;
    bufsz:minBlockLength 64 ;
    bufsz:maxBlockLength 8192 ;
    lv2:extensionData work:interface ;

    lv2:minorVersion 28;
    lv2:microVersion 3;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_cabinet#gui>;

rdfs:comment """

""";

    lv2:port  [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "CLevel" ;
        lv2:name "Cabinet";
        lv2:default 1.0 ;
        lv2:minimum 0.5 ;
        lv2:maximum 5.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "CBass" ;
        lv2:name "Bass";
        lv2:default 0.0 ;
        lv2:minimum -1e+01 ;
        lv2:maximum 1e+01 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "CTreble" ;
        lv2:name "Treble";
        lv2:default 0.0 ;
        lv2:minimum -1e+01 ;
        lv2:maximum 1e+01 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
