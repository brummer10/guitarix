        atom:bufferType atom:Sequence ;
        atom:supports <http://lv2plug.in/ns/ext/patch#Message> ;
        lv2:index 5 ;
        lv2:symbol "notify" ;
        lv2:name "Notify" ;
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "SCHEDULE" ;
        lv2:name "SCHEDULE";
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:notOnGUI;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 7 ;
        lv2:symbol "out" ;
        lv2:name "Out" ;
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 8 ;
        lv2:symbol "in" ;
        lv2:name "in" ;
    ] .


<http://guitarix.sourceforge.net/plugins/gx_cabinet#gui>
  a guiext:GtkUI;
  guiext:binary <gx_cabinet_gui.so>;
  guiext:requiredFeature guiext:makeResident;
  lv2:extensionData work:interface ;
  guiext:portNotification [
        guiext:plugin <http://guitarix.sourceforge.net/plugins/gx_cabinet#GUITARIX>;
        lv2:symbol "notify" ;
        guiext:notifyType atom:Blank
  ] .
