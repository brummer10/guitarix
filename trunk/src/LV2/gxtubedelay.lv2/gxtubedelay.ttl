@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Hermann Meyer" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gxtubedelay>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "GxRedeyeFx" .

<http://guitarix.sourceforge.net/plugins/gxtubedelay#tubedelay>
    a lv2:Plugin ,
        lv2:DelayPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxTubeDelay";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gxtubedelay> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    
  guiext:ui <http://guitarix.sourceforge.net/plugins/gxtubedelay#gui>;
  
    lv2:minorVersion 34;
    lv2:microVersion 0;
    
rdfs:comment """

...

""";

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "drive" ;
        lv2:name "Drive";
        lv2:default 0.5 ;
        lv2:minimum  0.0;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "delay" ;
        lv2:name "Delay";
        lv2:default 160.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 2500.0 ;
        unit:unit unit:ms ;
        lv2:portProperty <http://lv2plug.in/ns/ext/port-props#logarithmic> ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "feedback" ;
        lv2:name "Feedback";
        lv2:default 0.0 ;
        lv2:minimum 0.01 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "level" ;
        lv2:name "Level";
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1.0 ;
    ], [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "gain" ;
        lv2:name "Gain";
        lv2:default 0.2 ;
        lv2:minimum  0.0;
        lv2:maximum 1.0 ;
    ] ,  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 5 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "in" ;
        lv2:name "in"
    ] .

<http://guitarix.sourceforge.net/plugins/gxtubedelay#gui>
  a guiext:GtkUI;
  guiext:binary <gxtubedelay_gui.so>;
  guiext:binary <gxtubedelay_gui.so>;
  guiext:requiredFeature guiext:makeResident .
