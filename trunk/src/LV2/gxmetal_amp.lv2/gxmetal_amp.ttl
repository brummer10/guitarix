@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix bufsz: <http://lv2plug.in/ns/ext/buf-size#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix mod: <http://portalmod.com/ns/modgui#> .

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gxmetal_amp>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "GxMetalAmp" .

<http://guitarix.sourceforge.net/plugins/gxmetal_amp#metal_amp>
    a lv2:Plugin ,
        lv2:SimulatorPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxMetalAmp";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gxmetal_amp> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:requiredFeature bufsz:boundedBlockLength ;
    bufsz:minBlockLength 64 ;
    bufsz:maxBlockLength 8192 ;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gxmetal_amp#gui>;
  
    lv2:minorVersion 28;
    lv2:microVersion 3;

    lv2:port  [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "TONE" ;
        lv2:name "TONE";
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "DRIVE" ;
        lv2:name "DRIVE";
        lv2:default 10.5 ;
        lv2:minimum 1.0 ;
        lv2:maximum 20.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "PREGAIN" ;
        lv2:name "PREGAIN";
        lv2:default 0.0 ;
        lv2:minimum -2e+01 ;
        lv2:maximum 2e+01 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "GAIN1" ;
        lv2:name "GAIN1";
        lv2:default 0.0 ;
        lv2:minimum -2e+01 ;
        lv2:maximum 2e+01 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 4 ;
        lv2:symbol "out" ;
        lv2:name "Out" ;
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 5 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] ;
    mod:gui [ 
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/gxmetal_amp.html>;
        mod:templateData <modgui/gxmetal_amp.json>;
        mod:screenshot <modgui/gxmetal_amp.png>;
        mod:thumbnail <modgui/gxmetal_amp-thumb.png>;
    ] .

<http://guitarix.sourceforge.net/plugins/gxmetal_amp#gui>
  a guiext:GtkUI;
  guiext:binary <gxmetal_amp_gui.so>;
  guiext:requiredFeature guiext:makeResident .
  
